  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "8",
    "babel-plugin-module-resolver": "2.7",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.1",
    "eslint": "4.6",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-import-resolver-babel-module": "2.2",
    "eslint-plugin-import": "2.2",
    "mocha": "^3.4.2",
