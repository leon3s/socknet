  "description": "socket.io library with hook and argument validations",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && babel src --presets babel-preset-stage-2 --out-dir dist",
